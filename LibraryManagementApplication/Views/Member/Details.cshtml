@model LibraryManagementApplication.Data.Models.Member

<h2>Member Details</h2>

<dl class="row">
    <dt class="col-sm-2">MemberId</dt>
    <dd class="col-sm-10">@Model.MemberId</dd>

    <dt class="col-sm-2">Name</dt>
    <dd class="col-sm-10">@Model.Name</dd>

    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.Email</dd>

    <dt class="col-sm-2">ContactInfo</dt>
    <dd class="col-sm-10">@Model.ContactInfo</dd>

    <dt class="col-sm-2">MembershipDate</dt>
    <dd class="col-sm-10">@Model.MembershipDate.ToShortDateString()</dd>
</dl>

<a class="btn btn-secondary" asp-action="Index">Back to List</a>